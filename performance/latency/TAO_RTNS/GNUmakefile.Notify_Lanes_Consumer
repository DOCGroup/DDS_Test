  DEPLIBS = $(foreach lib, TAO_RT_Notification TAO_RTCORBA TAO_NotifyTests TAO_CosEvent_Serv TAO_IFR_Client TAO_DynamicInterface TAO_Messaging TAO_PI TAO_CodecFactory TAO_Valuetype TAO_CosNaming TAO_CosNotification_Serv TAO_DynamicAny TAO_ETCL TAO_Svc_Utils TAO_CosNotification_Skel TAO_CosEvent_Skel TAO_PortableServer TAO_CosNotification TAO_CosEvent TAO_AnyTypeCode TAO ACE , $(foreach libpath, . $(ACE_ROOT)/lib, $(wildcard $(libpath)/lib$(lib).a)))
endif

$(BIN): $(addprefix $(VDIR), $(OBJS)) $(DEPLIBS)
ifndef kylix
	$(LINK.cc) $(LDFLAGS) $(CC_OUTPUT_FLAG) $@ $^ $(VLDLIBS) $(POSTLINK)
else
	$(LINK.cc) $(LDFLAGS) $(CC_OUTPUT_FLAG) $(VLDLIBS) $(BORINITEXEOBJ) $(POSTLINK) $^, $@,,
endif
endif

