LDLIBS = -lTAO_RT_Notification -lTAO_RTCORBA -lTAO_NotifyTests -lTAO_CosEvent_Serv -lTAO_IFR_Client -lTAO_DynamicInterface -lTAO_Messaging -lTAO_PI -lTAO_CodecFactory -lTAO_Valuetype -lTAO_CosNaming -lTAO_CosNotification_Serv -lTAO_DynamicAny -lTAO_ETCL -lTAO_Svc_Utils -lTAO_CosNotification_Skel -lTAO_CosEvent_Skel -lTAO_PortableServer -lTAO_CosNotification -lTAO_CosEvent -lTAO_AnyTypeCode -lTAO -lACE
TAO_IDL = $(ACE_ROOT)/bin/tao_idl
TAO_IDL_DEP = $(ACE_ROOT)/bin/tao_idl$(EXEEXT)
TAO_IDLFLAGS = -Ge 1 -Wb,pre_include=ace/pre.h -Wb,post_include=ace/post.h -I$(TAO_ROOT)

