project {
  Define_Custom(NDDSTypeSupport) {
    automatic             = 1
    command               = $(NDDSHOME)/scripts/nddsgen
    dependent             = $(NDDSHOME)/scripts/nddsgen
    source_outputext      = .cxx
    keyword ndds_ts_flags = commandflags
  }

  ndds_ts_flags += -language C++ -d . -optimization 2 -replace
}
